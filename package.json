    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "fix": "gts fix",
    "prepare": "npm run build",
    "posttest": "npm run lint",
    "build": "npm run compile:esm && npm run compile:cjs"
  },
  "keywords": [
    "gdpr",
    "cookies",
    "react hooks",
    "react"
  ],
  "homepage": "https://github.com/bring-shrubbery/use-cookie-consent",
