{
  "name": "use-cookie-consent",
  "version": "0.1.7",
  "description": "React hook for managing GDPR cookie consent state.",
  "main": "./build/cjs/index.js",
  "types": "./build/cjs/index.d.ts",
  "module": "./build/esm/index.js",
  "files": [
    "build/"
  ],
  "scripts": {
    "test": "jest --passWithNoTests",
    "lint": "gts lint",
    "clean": "rimraf build",
