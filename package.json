  },
  "author": {
    "name": "Antoni Silvestrovič",
    "email": "antoni.silvestrovic@gmail.com",
    "url": "http://antoni.ai/"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^14.11.2",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "gts": "^3.1.0",
    "jest": "^27.0.1",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.1",
